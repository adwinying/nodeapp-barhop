{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-console": 0,
        "consistent-return": 0,
        "no-param-reassign": [2, { "props": false }]
    },
    "env": {
    	"browser": true,
    	"node": true
    }
}